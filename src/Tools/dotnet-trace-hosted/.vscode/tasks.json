{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "restore",
            "command": "dotnet",
            "type": "process",
            "args": [
                "restore",
                "${workspaceFolder}/dotnet-trace-hosted.csproj",
                "--packages",
                "${workspaceFolder}/../../../artifacts/packages"
            ],
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": true,
                "clear": true,
            },
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build",
            "command": "./build.cmd",
            "type": "shell",
            "args": [
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": "$msCompile"
        }
    ]
}